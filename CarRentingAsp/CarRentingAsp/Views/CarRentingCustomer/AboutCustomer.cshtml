    <link rel="stylesheet" href="~/css/magnific-popup.css">

    

    <link rel="stylesheet" href="~/css/bootstrap-datepicker.css">
    <link rel="stylesheet" href="~/css/jquery.timepicker.css">

    
    <link rel="stylesheet" href="~/css/flaticon.css">
    <link rel="stylesheet" href="~/css/icomoon.css">
    <link rel="stylesheet" href="~/css/style.css">
  </head>
	
  <body>
  <header>
    <!--Nav-->
	  <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
	    <div class="container">
	      <a class="navbar-brand">Car<span> Renting</span></a>
	      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
	        <span class="oi oi-menu"></span> Menu
	      </button>
           
	      <div class="collapse navbar-collapse" id="ftco-nav">
	        <ul class="navbar-nav ml-auto">
              <li class="nav-item"><a class="nav-link text-dark" asp-controller="CarRentingCustomer" asp-action="IndexCustomer">Home</a></li>
	          <li class="nav-item"><a class="nav-link text-dark" asp-controller="CarRentingCustomer" asp-action="AboutCustomer">About</a></li>
	          <li class="nav-item"><a class="nav-link text-dark" asp-controller="CarRentingCustomer" asp-action="ServiceCustomer">Services</a></li>
	          <li class="nav-item"><a class="nav-link text-dark" asp-controller="CarRentingCustomer" asp-action="PricingCustomer">Pricing</a></li>
	          <li class="nav-item"><a class="nav-link text-dark" asp-controller="CarRentingCustomer" asp-action="CarCustomer">Car</a></li>    
	          <li class="nav-item"><a class="nav-link text-dark" asp-controller="CarRentingCustomer" asp-action="ContactCustomer">Contact</a></li>  
              <li class="nav-item"><a class="nav-link text-dark" asp-controller="CarRentingCustomer" asp-action="ReviewCustomer">Review</a></li>
              <partial name="_LoginPartial" />
	        </ul>
	      </div>
	    </div>
	  </>
    <!-- End Nav -->
    </header>
    
    <section class="hero-wrap hero-wrap-2 js-fullheight" style="background-image: url('../images/bg_3.jpg');" data-stellar-background-ratio="0.5">
      <div class="overlay"></div>
      <div class="container">
        <div class="row no-gutters slider-text js-fullheight align-items-end justify-content-start">
          <div class="col-md-9 ftco-animate pb-5">
          	<p class="breadcrumbs"><span class="mr-2"><a href="@Url.Action("IndexGuest","CarRentingGuest")">Home <i class="ion-ios-arrow-forward"></i></a></span> <span>About <i class="ion-ios-arrow-forward"></i></span></p>
            <h1 class="mb-3 bread">About</h1>
          </div>
        </div>
      </div>
    </section>

   <section class="ftco-section ftco-about">
			<div class="container">
				<div class="row no-gutters">
					<div class="col-md-6 p-md-5 img img-2 d-flex justify-content-center align-items-center" style="background-image: url(images/about.jpg);">
					</div>
					<div class="col-md-6 wrap-about ftco-animate">
	          <div class="heading-section heading-section-white pl-md-5">
	          	<span class="subheading">About us</span>
	            <h2 class="mb-4">Welcome to Carbook</h2>

	            <p>Car Renting is a specialist car rental agency located in Craiova near to airport.We offer short term as well as long car renting.We offer car rental services to a variety of customers ranging from tourists, business companies, organization, and institutions undertaking their daily activities in Craiova.Rental is willing to go that extra to make your holiday/honeymoon in Romania relaxing and enjoyable. </p>
	            <p>Our cars are regulary serviced and are well maintained cars.</p>
				<p>Our rental prices are inclusive of insurance, taxes and our breakdown service.All our cars have valid road license and insurance.In case of a breakdown due to mechanical failure, a replacement vehicle will be delivered.</p>
	            
	          </div>
					</div>
				</div>
			</div>
		</section>



    

       <section class="ftco-counter ftco-section img bg-light" id="section-counter">
			<div class="overlay"></div>
    	<div class="container">
    		<div class="row">
          <div class="col-md-6 col-lg-3 justify-content-center counter-wrap ftco-animate">
            <div class="block-18">
              <div class="text text-border d-flex align-items-center">
                <strong class="number" data-number="60">0</strong>
                <span>Year <br>Experienced</span>
              </div>
            </div>
          </div>
          <div class="col-md-6 col-lg-3 justify-content-center counter-wrap ftco-animate">
            <div class="block-18">
              <div class="text text-border d-flex align-items-center">
                <strong class="number" data-number="1090">0</strong>
                <span>Total <br>Cars</span>
              </div>
            </div>
          </div>
          <div class="col-md-6 col-lg-3 justify-content-center counter-wrap ftco-animate">
            <div class="block-18">
              <div class="text text-border d-flex align-items-center">
                <strong class="number" data-number="2590">0</strong>
                <span>Happy <br>Customers</span>
              </div>
            </div>
          </div>
          <div class="col-md-6 col-lg-3 justify-content-center counter-wrap ftco-animate">
            <div class="block-18">
              <div class="text d-flex align-items-center">
                <strong class="number" data-number="67">0</strong>
                <span>Total <br>Branches</span>
              </div>
            </div>
          </div>
        </div>
    	</div>
    </section>	
     <!--Footer-->
    <footer class="ftco-footer ftco-bg-dark ftco-section">
      <div class="container">
        <div class="row mb-5">
          <div class="col-md">
            <div class="ftco-footer-widget mb-4">
              <h2 class="ftco-heading-2"><a href="@Url.Action("IndexGuest","CarRentingGuest")" class="logo">Car<span> Renting</span></a></h2>
              <ul class="ftco-footer-social list-unstyled float-md-left float-lft mt-5">
                <li class="ftco-animate"><a href="#"><span class="icon-twitter"></span></a></li>
                <li class="ftco-animate"><a href="#"><span class="icon-facebook"></span></a></li>
                <li class="ftco-animate"><a href="#"><span class="icon-instagram"></span></a></li>
              </ul>
            </div>
          </div>
          <div class="col-md">
            <div class="ftco-footer-widget mb-4 ml-md-5">
              <h2 class="ftco-heading-2">Information</h2>
              <ul class="list-unstyled">
                <li><a href="@Url.Action("AboutGuest","CarRentingGuest")" class="py-2 d-block">About</a></li>
                <li><a href="@Url.Action("ServiceGuest","CarRentingGuest")" class="py-2 d-block">Services</a></li>
                <li><a href="@Url.Action("PricingGuest","CarRentingGuest")" class="py-2 d-block">Best Price Guarantee</a></li>
              </ul>
            </div>
          </div>
          <div class="col-md">
             <div class="ftco-footer-widget mb-4">
              <h2 class="ftco-heading-2">Customer Support</h2>
              <ul class="list-unstyled">
                <li><a href="@Url.Action("ContactGuest","CarRentingGuest")" class="py-2 d-block">Contact Us</a></li>
              </ul>
            </div>
          </div>
          <div class="col-md">
            <div class="ftco-footer-widget mb-4">
            	<h2 class="ftco-heading-2">Have a Questions?</h2>
            	<div class="block-23 mb-3">
	              <ul>
	                <li><span class="icon icon-map-marker"></span><span class="text">Str. Alexandru Ioan Cuza 13, Craiova, Romania</span></li>
	                <li><span class="icon icon-phone"></span><span class="text">+40 0768858511</span></li>
	                <li><span class="icon icon-envelope"></span><span class="text">support@yahoo.com</span></li>
	              </ul>
	            </div>
            </div>
          </div>
        </div>
        <div class="row">
          <div class="col-md-12 text-center">
          </div>
        </div>
      </div>
    </footer>
    <!--End Footer-->
    
  

  <!-- loader -->
  <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee"/><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00"/></svg></div>

